<%- include("partials/header") -%>
<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="mb-3">
    <label for="postTitle" class="form-label">Title</label>
    <input type="text" name="postTitle" class="form-control" />
  </div>
  <div class="mb-3">
    <label for="postBody" class="form-label">Post</label>
    <textarea name="postBody" rows="5" cols="30" class="form-control"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-dark">Publish</button>
</form>


<%- include("partials/footer") -%>
